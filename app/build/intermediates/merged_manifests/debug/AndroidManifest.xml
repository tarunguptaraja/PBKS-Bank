<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.basicbankingapp"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="19"
        android:targetSdkVersion="30" />

    <application
        android:allowBackup="true"
        android:appComponentFactory="androidx.core.app.CoreComponentFactory"
        android:debuggable="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:testOnly="true"
        android:theme="@style/Theme.BasicBankingApp" >
        <activity
            android:name="com.example.basicbankingapp.Users_list"
            android:parentActivityName="com.example.basicbankingapp.MainActivity" />
        <activity
            android:name="com.example.basicbankingapp.User_data"
            android:parentActivityName="com.example.basicbankingapp.Users_list" />
        <activity
            android:name="com.example.basicbankingapp.Transaction_history"
            android:parentActivityName="com.example.basicbankingapp.MainActivity" />
        <activity android:name="com.example.basicbankingapp.send_to_user" />
        <activity android:name="com.example.basicbankingapp.MainActivity" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>